<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AsyncCommandMvvm.MainPage"
             Title="AsyncCommandMvvm">

    <VerticalStackLayout>
        <Label Text="Async command and initialize"/>
        <Button Text="Click" Command="{Binding SaveCommand}"/>
        <Button Text="Error Command" Command="{Binding OtherCommand}"/>
        <Label Text="{Binding Title}"/>
    </VerticalStackLayout>

</ContentPage>
