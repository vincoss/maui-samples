<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="FlyoutPage_Samples.MainPage"
             xmlns:views="clr-namespace:FlyoutPage_Samples.Views;assembly=FlyoutPage_Samples">

    <FlyoutPage.Flyout>
        <views:FlyoutMenuPage x:Name="flyoutPage" />
    </FlyoutPage.Flyout>

    <FlyoutPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <views:ContactsPage />
            </x:Arguments>
        </NavigationPage>
    </FlyoutPage.Detail>

</FlyoutPage>
