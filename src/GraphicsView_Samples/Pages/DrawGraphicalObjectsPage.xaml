<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="GraphicsView_Samples.Pages.DrawGraphicalObjectsPage"
             xmlns:drawable="clr-namespace:GraphicsView_Samples.Pages"
             Title="DrawGraphicalObjectsPage">

    <ContentPage.Resources>
        <drawable:DrawGraphicalObjectsPageDrawable x:Key="drawable" />
    </ContentPage.Resources>

    <GraphicsView x:Name="canvas"
                  Drawable="{StaticResource drawable}"/>
    
</ContentPage>